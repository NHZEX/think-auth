{
    "name": "zxin/think-auth",
    "description": "think auth",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "auooru",
            "email": "auooru@outlook.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "doctrine/annotations": "^1.10",
        "symfony/polyfill-php80": "^1.18",
        "topthink/framework": "^6.0.3",
        "zxin/think-annotation": "dev-master",
        "zxin/utils": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8|^9"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "Zxin\\Think\\Auth\\": "src/"
        },
        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Zxin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "think": {
            "services": [
            ],
            "config": {
                "auth": "config/auth.php"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": false
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "url": "../think-annotation",
            "type": "path"
        }
    ]
}
