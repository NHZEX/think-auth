{
    "name": "zxin/think-auth",
    "description": "think auth",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "auooru",
            "email": "auooru@outlook.com"
        }
    ],
    "require": {
        "php": "~7.2",
        "ext-json": "*",
        "ext-redis": "*",
        "doctrine/annotations": "^1.10",
        "symfony/finder": "^4.2|^5.1",
        "symfony/polyfill-php80": "^1.18",
        "topthink/framework": "^6.0.3",
        "zxin/utils": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7|^8",
        "symfony/var-dumper": "^4.4|^5.0"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "Zxin\\Think\\Auth\\": "src/"
        },
        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Zxin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "think": {
            "services": [
                "Zxin\\Think\\Auth\\Service"
            ],
            "config": {
                "auth": "config/auth.php"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": false
    },
    "minimum-stability": "stable"
}
